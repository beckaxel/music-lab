{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.166.0",
  "pnpmVersion": "10.28.1",
  "nodeSupportedVersionRange": ">=24.11.1 <25.0.0",
  "approvedPackagesPolicy": {
     "reviewCategories": [
        "library",
        "application",
        "webpage"
      ],
      "ignoredNpmScopes": ["@types"]
  },
  "gitPolicy": { },
  "repository": {
    "url": "https://github.com/beckaxel/music-lab",
    "defaultBranch": "main",
    "defaultRemote": "origin"
  },
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": [],
    "preRushx": [],
    "postRushx": []
  },
  "variants": [],
  "telemetryEnabled": false,
  "allowedProjectTags": [ "lib", "app", "web", "core", "component" ],
  "projects": [
    {
      "packageName": "music-lab-core",
      "projectFolder": "libraries/music-lab-core",
      "versionPolicyName": "music-lab",
      "reviewCategory": "library",
      "tags": [ "lib", "core" ]
    }, {
      "packageName": "music-lab-components",
      "projectFolder": "libraries/music-lab-components",
      "versionPolicyName": "music-lab",
      "reviewCategory": "library",
      "tags": [ "lib", "core", "component" ]
    }, {
      "packageName": "music-lab-tuner",
      "projectFolder": "web/music-lab-tuner",
      "versionPolicyName": "music-lab",
      "reviewCategory": "library",
      "tags": [ "lib", "web" ]
    }
  ]
}
